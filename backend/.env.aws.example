# Ambiente AWS - PostgreSQL em Container
# COPIE ESTE ARQUIVO PARA .env.aws E AJUSTE OS VALORES

NODE_ENV=production

# Configuração do Banco de Dados PostgreSQL
DB_DIALECT=postgres
DB_HOST=postgres
DB_PORT=5432
DB_NAME=ang_database
DB_USER=ang_user
DB_PASSWORD=ALTERE_ESTA_SENHA_EM_PRODUCAO
DB_SSL=false
DB_LOGGING=false

# Outras configurações
PORT=3001
JWT_SECRET=ALTERE_ESTE_SECRET_EM_PRODUCAO
JWT_EXPIRATION=24h

# CORS - Frontend URL (ajuste com IP/domínio da AWS)
FRONT_ORIGIN=http://seu-ip-aws:8080
FRONTEND_URL=http://seu-ip-aws:8080

# Backend
BACKEND_ORIGIN=http://seu-ip-aws:3001

# Configuração do Swagger
SWAGGER_USER=admin
SWAGGER_PASS=ALTERE_ESTA_SENHA_EM_PRODUCAO
SWAGGER_SERVER_URL=http://seu-ip-aws:3001/api/v2
